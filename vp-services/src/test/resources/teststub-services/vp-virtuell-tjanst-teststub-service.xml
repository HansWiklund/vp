<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:spring="http://www.springframework.org/schema/beans"
    xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf"
    xmlns:https="http://www.mulesoft.org/schema/mule/https"
    xmlns="http://www.mulesoft.org/schema/mule/core"
    xsi:schemaLocation="
		http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/https http://www.mulesoft.org/schema/mule/https/current/mule-https.xsd
		http://www.mulesoft.org/schema/mule/cxf http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd" >

      <flow name="vp-virtuell-tjanst-teststub-flow" >
        <inbound-endpoint 
        	address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/tjanst1"  
        	connector-ref="VPProducerConnector" 
 			name="Tjanst1"
            exchange-pattern="request-response" 
            responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
        	<properties>
        		<spring:entry key="featureUseKeepAlive" value="false"/>
        	</properties>
        </inbound-endpoint>
 			
        <cxf:proxy-service
            namespace="urn:skl:tjanst1:rivtabp20"
            payload="envelope"
            service="Tjanst1Service"
            wsdlLocation="classpath:/schemas/interactions/tjanst1Interaction/tjanst1-1.0.wsdl" />

        <flow-ref name="vagval-dynamic-routing-flow" />
    </flow>
</mule>
