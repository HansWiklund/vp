<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:spring="http://www.springframework.org/schema/beans"
    xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf"
    xmlns:https="http://www.mulesoft.org/schema/mule/https"
    xmlns:http="http://www.mulesoft.org/schema/mule/http"
    xmlns="http://www.mulesoft.org/schema/mule/core"
    xsi:schemaLocation="
		http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-current.xsd
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/https http://www.mulesoft.org/schema/mule/https/current/mule-https.xsd
		http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd
		http://www.mulesoft.org/schema/mule/cxf http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd" >

    <flow name="vp-virtuell-tjanst-teststub-flow" >
       
        <composite-source>
      	  	<https:inbound-endpoint
        	    address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/tjanst1"
           		connector-ref="VPProducerConnector"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
            	<properties>
        			<spring:entry key="featureUseKeepAlive" value="true"/>
              		<spring:entry key="featureResponseTimeout" value="${TEST_NORMAL_TIMEOUT_MS}"/>
        		</properties>
        	</https:inbound-endpoint>
        	<http:inbound-endpoint
        	    address="http://${TP_HOST}:${TP_PORT_HTTP}/${TP_BASE_URI}/tjanst1"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="filterResponseHeaders">
              <properties>
              <spring:entry key="featureResponseTimeout" value="${TEST_NORMAL_TIMEOUT_MS}"/>
            </properties>
        	</http:inbound-endpoint>
         </composite-source>
 			
        <cxf:proxy-service
            namespace="urn:skl:tjanst1:rivtabp20"
            payload="envelope"
            service="Tjanst1Service"
            wsdlLocation="classpath:/schemas/interactions/tjanst1Interaction/tjanst1-1.0.wsdl" />

        <flow-ref name="vagval-dynamic-routing-flow" />
    </flow>

    <flow name="vp-virtuell-tjanst-teststub-short-timeout-flow" >
       
        <composite-source>
      	  	<https:inbound-endpoint
        	    address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/tjanst1-short-timeout"
           		connector-ref="VPProducerConnector"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
            	<properties>
        			<spring:entry key="featureUseKeepAlive" value="true"/>
              		<spring:entry key="featureResponseTimeout" value="${TEST_SHORT_TIMEOUT_MS}"/>
        		</properties>
        	</https:inbound-endpoint>
        	<http:inbound-endpoint
        	    address="http://${TP_HOST}:${TP_PORT_HTTP}/${TP_BASE_URI}/tjanst1-short-timeout"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="filterResponseHeaders">
              <properties>
              <spring:entry key="featureResponseTimeout" value="${TEST_SHORT_TIMEOUT_MS}"/>
            </properties>
        	</http:inbound-endpoint>
         </composite-source>
 			
        <cxf:proxy-service
            namespace="urn:skl:tjanst1:rivtabp20"
            payload="envelope"
            service="Tjanst1Service"
            wsdlLocation="classpath:/schemas/interactions/tjanst1Interaction/tjanst1-1.0.wsdl" />

        <flow-ref name="vagval-dynamic-routing-flow" />
    </flow>

    <flow name="vp-virtuell-tjanst-teststub-long-timeout-flow" >
       
        <composite-source>
      	  	<https:inbound-endpoint
        	    address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/tjanst1-long-timeout"
           		connector-ref="VPProducerConnector"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
            	<properties>
        			<spring:entry key="featureUseKeepAlive" value="true"/>
              		<spring:entry key="featureResponseTimeout" value="${TEST_LONG_TIMEOUT_MS}"/>
        		</properties>
        	</https:inbound-endpoint>
        	<http:inbound-endpoint
        	    address="http://${TP_HOST}:${TP_PORT_HTTP}/${TP_BASE_URI}/tjanst1-long-timeout"
            	exchange-pattern="request-response" 
            	responseTransformer-refs="filterResponseHeaders">
              <properties>
              <spring:entry key="featureResponseTimeout" value="${TEST_LONG_TIMEOUT_MS}"/>
            </properties>
        	</http:inbound-endpoint>
         </composite-source>
 			
        <cxf:proxy-service
            namespace="urn:skl:tjanst1:rivtabp20"
            payload="envelope"
            service="Tjanst1Service"
            wsdlLocation="classpath:/schemas/interactions/tjanst1Interaction/tjanst1-1.0.wsdl" />

        <flow-ref name="vagval-dynamic-routing-flow" />
    </flow>


</mule>
