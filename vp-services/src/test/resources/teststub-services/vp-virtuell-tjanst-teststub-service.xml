      <flow name="vp-virtuell-tjanst-teststub-flow" >
        <inbound-endpoint connector-ref="VPProducerConnector" address="https://${tp.host}:${tp.port}/${tp.baseUri}/tjanst1"  
 			name="Tjanst1" />

        <cxf:jaxws-service
            serviceClass="se.skl.tp.vp.vagvalrouter.producer.VpFullServiceTestProducer"
			/>
        
        <outbound-endpoint address="vm://vagval-router" >
        </outbound-endpoint>    
        
    </flow>
</mule>
