<xs:schema 
	targetNamespace="urn:riv:domain:subdomain:GetProductDetailResponder:1"
	xmlns:tns="urn:riv:domain:subdomain:GetProductDetailResponder:1"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	attributeFormDefault="unqualified"
	elementFormDefault="qualified"
	version="1.0">

	<xs:complexType name="product">
		<xs:sequence>
			<xs:element minOccurs="0" name="description" type="xs:string" />
			<xs:element name="height" type="xs:int" />
			<xs:element minOccurs="0" name="id" type="xs:string" />
			<xs:element name="width" type="xs:int" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="getProductDetailElem" type="tns:getProductDetailType" />
	<xs:complexType name="getProductDetailType">
		<xs:sequence>
			<xs:element minOccurs="0" name="productId" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="getProductDetailResponse" type="tns:getProductDetailResponse" />
	<xs:complexType name="getProductDetailResponse">
		<xs:sequence>
			<xs:element minOccurs="0" name="product" type="tns:product" />
		</xs:sequence>
	</xs:complexType>

</xs:schema>
