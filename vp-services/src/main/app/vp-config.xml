        <spring:import resource="classpath:vp-common.xml"/>
<!-- 
        <spring:import resource="classpath*:services/*-service.xml"/>
 -->
        
        <!-- Import virtual services -->
        <spring:import resource="classpath*:tp2-service-mule-descriptor.xml"/>
        
    </spring:beans>

</mule>
