
	<!-- Flow -->
	<flow name="%s-flow" >

        <https:inbound-endpoint
            address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/%s"
            connector-ref="VPProducerConnector"
            exchange-pattern="request-response" 
            responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
            <properties>
        		<spring:entry key="featureUseKeepAlive" value="${feature.keepalive.%s:${feature.keepalive}}"/>
        	</properties>
        </https:inbound-endpoint>

        <cxf:proxy-service
            namespace="%s"
            payload="envelope"
            service="%s"
            wsdlLocation="classpath:%s" />

        <flow-ref name="vagval-dynamic-routing-flow" />
    </flow>