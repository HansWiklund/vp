	
	<!-- Flow -->
	<flow name="%1$s-flow">
		<composite-source>
			<https:inbound-endpoint
				address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/%2$s"
				connector-ref="VPProducerConnector" exchange-pattern="request-response"
				responseTransformer-refs="featureKeepAliveResponse filterResponseHeaders">
				<properties>
					<spring:entry key="featureUseKeepAlive"
						value="${feature.keepalive.%3$s:${feature.keepalive}}" />
				</properties>
			</https:inbound-endpoint>
	
			<http:inbound-endpoint
				address="http://${TP_HOST}:${TP_PORT_HTTP}/${TP_BASE_URI}/%2$s"
				exchange-pattern="request-response" responseTransformer-refs="filterResponseHeaders">
			</http:inbound-endpoint>
		</composite-source>
	
		<cxf:proxy-service namespace="%4$s" payload="envelope"
			service="%5$s" wsdlLocation="classpath:%6$s" />
	
		<flow-ref name="vagval-dynamic-routing-flow" />
	</flow>
