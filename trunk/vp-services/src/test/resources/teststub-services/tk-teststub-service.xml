<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:spring="http://www.springframework.org/schema/beans"
    xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf"
    xmlns="http://www.mulesoft.org/schema/mule/core"
    xsi:schemaLocation="
		http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-current.xsd
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/cxf http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd" >

    <description>
		Test konfiguration för att simulera Tjänstekatalogen
    </description>

    <spring:bean
        name="vagvalInfo"
        class="se.skl.tp.vp.vagvalrouter.VagvalInfo" />

    <flow name="tk-teststub-flow" >

        <inbound-endpoint address="${TP_SOKVAGVALSINFO_URL}" />

        <cxf:jaxws-service
            serviceClass="se.skl.tp.vp.vagvalagent.SokVagvalsInfoTestStub"
            wsdlLocation="classpath:/VAGVAL_2_0_0/interactions/vagvalsInfo/wrapped-vagvalsinfo-sokvagvalsinfo-2.0.wsdl" />

        <component>
            <singleton-object class="se.skl.tp.vp.vagvalagent.SokVagvalsInfoTestStub" >
                <property
                    key="vagvalInputs"
                    value-ref="vagvalInputs" />
            </singleton-object>
        </component>
    </flow>

</mule>